{
  "name": "cognito-local",
  "version": "1.0.0",
  "license": "MIT",
  "bin": "dist/bin/start.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "tsc",
    "integration-testOLD": "jest --config tests/jest.config.js",
    "integration-test:watchOLD": "jest --config tests/jest.config.js --watch",
    "lintOLD": "eslint --cache src/**/*.ts && tsc --noEmit",
    "start:dev": "cross-env COGNITO_LOCAL_DEVMODE=1 ts-node src",
    "start:prod": "node dist/index.js",
    "start:debugOLD": "COGNITO_LOCAL_DEVMODE=1 DEBUG=1 node -r ts-node/register --inspect=9230 --enable-source-maps src",
    "start:watchOLD": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.16.0",
    "@types/aws-lambda": "8.10.85",
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash.mergewith": "4.6.6",
    "@types/node": "16.11.11",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.3",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "jest": "27.4.3",
    "jest-extended": "3.0.1",
    "nodemon": "2.0.3",
    "prettier": "2.5.1",
    "rimraf": "4.1.1",
    "supertest": "6.1.6",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@aws-crypto/client-node": "3.1.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "2.1297.0",
    "body-parser": "1.20.1",
    "boxen": "7.0.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "jsonwebtoken": "9.0.0",
    "lodash.mergewith": "4.6.2",
    "pino": "8.8.0",
    "pino-http": "8.3.1",
    "pino-pretty": "9.1.1",
    "short-uuid": "4.2.2",
    "stormdb": "0.6.0",
    "uuid": "9.0.0"
  }
}
